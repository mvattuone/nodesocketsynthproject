<html>
	<head>
		<title>Collaborative Drum Machine</title>
		<link rel="stylesheet" href="./stylesheets/main.css">
		<script src="/socket.io/socket.io.js"></script>
	</head>
	<body>
		<div id="container">
		</div>
		<ul class="pages">
			<li class="chat page">
				<div class="chatArea">
					<ul class="messages">
						<li id="roomname" class="log">Room: </li>
					</ul>
				</div>
				<input class="inputMessage" placeholder="Type here..."/>
			</li>
			<li class="login page">
				<div class="form">
					<h3 class="nametitle">Welcome to sequenSync, What's your name?</h3>
					<h3 class="roomtitle">Load a room, or create a new one..</h3>
					<input class="usernameInput" type="text" maxlength="14" />
					<input class="roomnameInput" type="text" maxlength="14" />
				</div>
			</li>
		</ul>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.7.0/underscore.js"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular.min.js"></script>
		<script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
		<script src="./javascripts/three.js"></script>
		<script src="./javascripts/soundLoader.js"></script>
    <script src="./javascripts/machineBuilder.js"></script>
		<script src="./javascripts/player.js"></script>
		<script src="./javascripts/chat.js"></script>
		<script>
			App.render();
		</script>
	</body>
</html>
